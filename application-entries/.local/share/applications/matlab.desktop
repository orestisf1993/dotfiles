[Desktop Entry]
Version=1.0
Type=Application
Icon=matlab
Name=Matlab
Comment=The Language of Technical Computing
Exec=i3-msg "workspace 5; append_layout $HOME/.i3/layouts/matlab.json"&&matlab -desktop -nosplash
Terminal=false
Categories=Development;
StartupNotify=true
StartupWMClass=sun-awt-X11-XFramePeer
X-Ayatana-Desktop-Shortcuts=New_Window;

Name[en_US]=MATLAB

[New_Window Shortcut Group]
Name=start a new MATLAB instance
Exec=matlab -desktop
